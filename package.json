{
  "name": "remix-app-template",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "yarn eth-sdk && run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "format": "prettier --write .",
    "generate:css": "npx tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "postinstall": "remix setup node",
    "start": "cross-env NODE_ENV=production remix-serve build",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "export PORT=8811; start-server-and-test dev http://localhost:8811 \"cypress run\"",
    "typecheck": "tsc -b",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "storybook": "export NODE_OPTIONS=--openssl-legacy-provider; start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@0xsequence/multicall": "^0.43.3",
    "@dethcrypto/eth-sdk": "^0.5.0",
    "@dethcrypto/eth-sdk-client": "^0.2.1",
    "@ethersproject/providers": "^5.7.2",
    "@faker-js/faker": "^7.6.0",
    "@remix-run/react": "^1.12.0",
    "@remix-run/vercel": "^1.12.0",
    "@testing-library/dom": "^8.19.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react-gtm-module": "^2.0.1",
    "@vitejs/plugin-react": "^3.0.0",
    "@web3-react/core": "^8.0.44",
    "@web3-react/metamask": "^8.0.44",
    "@web3-react/types": "^8.0.44",
    "bignumber.js": "^9.1.1",
    "binode": "^1.0.5",
    "c8": "^7.12.0",
    "clsx": "^1.2.1",
    "ethers": "^5.7.2",
    "graphql": "^16.6.0",
    "graphql-request": "^5.1.0",
    "human-readable-numbers": "^0.9.5",
    "logrocket": "^3.0.0",
    "lottie-react": "^2.3.1",
    "mathjs": "^11.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-gtm-module": "^2.0.11",
    "react-hot-toast": "^2.4.0",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.6.2",
    "react-svg-text": "^0.1.2",
    "react-timer-hook": "^3.0.5",
    "recharts": "^2.3.2",
    "remix": "^1.12.0",
    "start-server-and-test": "^1.14.0",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "^0.27.1"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@remix-run/dev": "^1.12.0",
    "@remix-run/eslint-config": "^1.12.0",
    "@remix-run/serve": "^1.12.0",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack4": "^6.5.15",
    "@storybook/manager-webpack4": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@typechain/ethers-v5": "^10.2.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-modal": "^3.13.1",
    "babel-loader": "^8.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.31.0",
    "eslint-plugin-storybook": "^0.6.8",
    "happy-dom": "^2.55.0",
    "msw": "^0.49.2",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14"
  }
}
